{
  "name": "@h3ro-dev/motion-mcp-server",
  "version": "0.1.0",
  "description": "A comprehensive Model Context Protocol (MCP) server for Motion API integration",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "motion-mcp": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CryptoJym/motion-mcp-server.git"
  },
  "keywords": [
    "mcp",
    "motion",
    "task-management",
    "project-management",
    "api",
    "ai-assistant"
  ],
  "author": "h3ro",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CryptoJym/motion-mcp-server/issues"
  },
  "homepage": "https://github.com/CryptoJym/motion-mcp-server#readme",
  "devDependencies": {
    "@types/node": "^24.0.4",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.13.1",
    "@types/better-sqlite3": "^7.6.13",
    "@types/sqlite3": "^3.1.11",
    "axios": "^1.10.0",
    "better-sqlite3": "^12.1.1",
    "dotenv": "^16.5.0",
    "sqlite3": "^5.1.7",
    "zod": "^3.25.67"
  }
}
